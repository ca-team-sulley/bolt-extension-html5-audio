{
    "name": "cainc/html5-audio",
    "description": "Upload audio files, then use a waveform or html5 based player to play audio files.",
    "type": "bolt-extension",
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Team Sulley",
            "email": "team-sulley@cainc.com"
        },
        {
            "name": "Pablo SÃ¡nchez",
            "email": "psanchezg@gmail.com"
        }
    ],
    "keywords": [
        "field",
        "audio",
        "backend"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Cainc\\Html5Audio\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Cainc\\Html5Audio\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Cainc\\Html5Audio\\Html5AudioExtension",
        "bolt-screenshots": [
            "screenshots/backend_audio.png",
            "screenshots/backend_waveform.png",
            "screenshots/frontend_audio.png",
            "screenshots/frontend_waveform.png"
        ]
    }
}
